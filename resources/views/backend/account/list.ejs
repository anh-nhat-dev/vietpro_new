<%- contentFor('head') %>

<%- contentFor('main') %>

    <div class="container_fluid">
        <div class="row">
            <div class="col-lg-12">
                <!-- USER DATA-->
                <div class="user-data m-b-30">
                    <h3 class="title-3 m-b-30">
                        <i class="zmdi zmdi-account-calendar"></i>Danh sách thành viên</h3>
                    <div class="table-responsive table-data">
                        <table class="table">
                            <thead>
                                <tr>
                                    <td>
                                        <label class="au-checkbox">
                                            <input type="checkbox">
                                            <span class="au-checkmark"></span>
                                        </label>
                                    </td>
                                    <td>id</td>
                                    <td>name</td>
                                    <td>role</td>
                                    <td>type</td>
                                    <td></td>
                                </tr>
                            </thead>
                            <tbody>

                            <% if (data !== undefined && data.length) { %>
                                <% data.forEach(function(user){ %>
                                    <%- include('component/table_list', user )%>
                                <% }) %>
                            <% } %>
                            </tbody>
                        </table>
                    </div>
                    <div class="user-data__footer">
                        <button class="au-btn au-btn-load">load more</button>
                    </div>
                </div>
                <!-- END USER DATA-->
            </div>
        </div>
    </div>

<%- contentFor('foot') %>
